{
  "name": "FRECL",
  "version": "1.0.0",
  "description": "BlueChilli's Front End Component Library",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache",
    "start": "cross-env NODE_ENV=development node server.js",
    "build-start": "cross-env NODE_ENV=development webpack -p --config webpack.config.js",
    "build-dev": "cross-env NODE_ENV=dev webpack -p --config webpack.production.config.js",
    "build-stg": "cross-env NODE_ENV=staging webpack -p --config webpack.production.config.js",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "build-server-dev": "cross-env NODE_ENV=dev webpack -p --config webpack.server.config.js",
    "build-server-stg": "cross-env NODE_ENV=staging webpack -p --config webpack.server.config.js",
    "build-server": "cross-env NODE_ENV=production webpack -p --config webpack.server.config.js",
    "docs": "react-docgen Frecl/* --out doc/doc.json --pretty"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BlueChilli/FRECL.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BlueChilli/FRECL/issues"
  },
  "homepage": "https://github.com/BlueChilli/FRECL",
  "jest": {
    "globals": {
      "window": true
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/node_modules/identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTestFramework.js",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/typescript-babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "axios": "^0.15.3",
    "classnames": "^2.2.5",
    "cross-env": "^3.1.3",
    "history": "3.2.1",
    "immutable": "^3.8.1",
    "lodash": "^4.13.0",
    "moment": "^2.13.0",
    "raygun4js": "^2.3.3",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.4.1",
    "react-addons-test-utils": "^15.3.2",
    "react-date-range": "^0.9.0",
    "react-dom": "^15.2.1",
    "react-dropzone": "^3.4.0",
    "react-geosuggest": "^1.25.0",
    "react-helmet": "^4.0.0",
    "react-onclickoutside": "^5.8.3",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.4",
    "react-select": "^1.0.0-rc.2",
    "react-style-guide": "^1.1.8",
    "recompose": "^0.20.2",
    "redux": "^3.5.2",
    "redux-immutablejs": "0.0.8",
    "redux-localstorage": "^0.4.0",
    "redux-promise-middleware": "^3.0.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.35",
    "@types/classnames": "0.0.32",
    "@types/immutable": "^3.8.6",
    "@types/jest": "^18.1.1",
    "@types/lodash": "^4.14.44",
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "@types/react-dropzone": "0.0.30",
    "@types/react-redux": "^4.4.35",
    "@types/recompose": "^0.20.2",
    "@types/redux-thunk": "^2.1.0",
    "autoprefixer": "^6.3.6",
    "awesome-typescript-loader": "^3.0.2",
    "babel-core": "^6.9.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "breakpoint-sass": "^2.7.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "clean-webpack-plugin": "^0.1.9",
    "config": "^1.20.3",
    "cookie": "^0.3.1",
    "css-loader": "^0.26.0",
    "enzyme": "^2.6.0",
    "express": "^4.13.4",
    "express-urlrewrite": "^1.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "identity-obj-proxy": "^3.0.0",
    "img-loader": "^1.3.1",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "my-local-ip": "^1.0.0",
    "node-sass": "^3.7.0",
    "open": "0.0.5",
    "postcss-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "redbox-react": "^1.2.5",
    "redux-logger": "^2.6.1",
    "sass-loader": "^4.0.2",
    "scut": "^1.4.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "susy": "^2.2.12",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.5",
    "typescript-babel-jest": "^0.1.8",
    "url-loader": "^0.5.7",
    "webfonts-generator": "^0.3.5",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-fail-plugin": "^1.0.5",
    "webpack-hot-middleware": "^2.10.0"
  }
}
